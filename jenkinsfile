pipeline{
    agent any
    stages{
        stage("Check out the repo"){
            steps{
                sh 'git clone https://github.com/shri69/java-code1'
            }
        }
            stage("Maven Compile"){
                steps{
                    sh 'MVN compile'
                }
            }
            stage("Maven Test"){
                steps{
                    sh 'mvn test'
                }
            }
            stage("Maven Package"){
                steps{
                    sh 'mvn package'
                }
            }
            stage("Post"){
            post{
                always{
                    echo "========always========"
                }
                success{
                    echo "========A executed successfully========"
                }
                failure{
                    echo "========A execution failed========"
                }
            }
        }
    }
}